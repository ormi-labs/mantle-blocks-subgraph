  "scripts": {
    "0xcodegen": "rm -rf generated && 0xgraph codegen -o generated",
    "0xbuild": "0xgraph build",
    "0xcreate": "0xgraph create mantle/blocks --node <HTTP://GRAPH_NODE_HOST:PORT> --access-token <API_KEY>",
    "0xdeploy": "0xgraph deploy mantle/blocks --node <HTTP://GRAPH_NODE_HOST:PORT> --ipfs <HTTP://IPFS_HOST:PORT>",
    "0xremove": "0xgraph remove mantle/blocks --access-token <API_KEY>"
  },
  "dependencies": {
    "@0xgraph/cli": <CLI_EXACT_VERSION>,
    "@graphprotocol/graph-ts": "0.26.0"
  }
}
